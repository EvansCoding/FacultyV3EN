
@{
    ViewBag.Title = "CategoryView";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}


<div id="data1">
    <div class="row">
        <div class="col-12">
            <div class="page-title-box">
                <h4 class="page-title">List Category</h4>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <div class="table-responsive" id="tableAjax">
                        @{
                            Html.RenderAction("LoadTable", "Category");
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


