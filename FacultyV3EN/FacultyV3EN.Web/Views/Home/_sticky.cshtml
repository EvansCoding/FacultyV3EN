@model List<FacultyV3EN.Core.Models.Entities.Sticky>
<div class="footer_bar d-flex flex-column flex-sm-row align-items-center">
    <div class="row" style="width: 100%">
        <div class="col-lg-3" style="padding-bottom: 10px">
            <div class="total_visitors">
                <span>User visitors: <span id="total_user_visitor"> @ViewBag.Total </span> |</span>
                <span>User online: <span id="user_online"> 1 </span></span>
            </div>
        </div>
        <div class="col-lg-7" style="padding-bottom: 10px">
            <div class="footer_copyright" style="text-align: center">
                <span>
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    Copyright &copy;
                    <script>document.write(new Date().getFullYear());</script> Faculty of Engineering and Technology
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                </span>
            </div>
        </div>
        <div class="col-lg-2" style="padding-right: 0px;">
            <div class="footer_social ml-sm-auto" >
                <ul class="menu_social">
                    <li class="menu_social_item"><a href="@Model[1].Meta_Value" target="_blank"><i class="fas fa-home"></i></a></li>
                    <li class="menu_social_item"><a href="@Model[3].Meta_Value" target="_blank"><i class="fab fa-youtube"></i></a></li>
                    <li class="menu_social_item"><a href="@Model[0].Meta_Value" target="_blank"><i class="fab fa-facebook-f"></i></a></li>
                </ul>
            </div>
        </div>
    </div>
</div>
